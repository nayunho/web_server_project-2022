      <a href="ETC_tip" style="color: black">기타학문</a>
      <a href="notice" style="color: black">공지사항</a>
    </div>
    
    <div id="main" style="display: inline-block; margin: 0 5px;">
      <button class="openbtn" onclick="openNav()">☰</button>  
    </div>
    
    <script>
    function openNav() {
      document.getElementById("mySidebar").style.width = "200px";
      document.getElementById("main").style.marginRight = "250px";
    }
    
    function closeNav() {
      document.getElementById("mySidebar").style.width = "0";
      document.getElementById("main").style.marginRight= "0";
    }
    </script>

    <!--content-->
    
        <div class="board_wrap">
            <div class="board_title">
                <strong>Etc</strong>
                <p>모르는 것을 물어보세요 !</p>
            </div>
            <div class="board_list_wrap">
                <div class="board_list">
                    <div class="top">
                        <div class="num">번호</div>
                        <div class="title">제목</div>
                        <div class="writer">글쓴이</div>
                        <div class="date">작성일</div>
                        
                    </div>
                    <% var count =0; %>
                    <% for (var i = loginfo.length-1; i >=0; i --) { %> 
                        <% if (loginfo[i].title != null){   %>
                            <% count+=1; %>
                            
                        <% } %>
                        
                    <% } %>   

                    <% for (var i = loginfo.length-1; i >=0; i --) { %> 
                        <% if (loginfo[i].id != null){   %>
                            
                            <% continue; %>
                            
                        <% } %>
                        <div>
                            
                            <div class="num"><%=count%></div>
                            <div class="title" style="text-align: center;"><a href="/ETC_qna_view"><%=loginfo[i].title %></a></div>
                            <div class="writer"><%=loginfo[i].nickname %></div>
                            <div class="date"><%=loginfo[i].date %></div>
                            <% count-=1; %>
                        </div>  
                    <% } %>

                    
                </div>
                <div class="board_page">
                    <a href="#" class="bt first"><<</a>
                    <a href="#" class="bt prev"><</a>
                    <a href="#" class="num on">1</a>
                    <a href="#" class="num">2</a>
                    <a href="#" class="num">3</a>
                    <a href="#" class="num">4</a>
                    <a href="#" class="num">5</a>
                    <a href="#" class="bt next">></a>
                    <a href="#" class="bt last">>></a>
                </div>
                <div class="bt_wrap">
                    <a href="/web" class="on">홈으로 돌아기기</a>
                    <a href="/ETC_qna_write" class="on">등록</a>
                    <!--<a href="#">수정</a>-->
                </div>
            </div>
        </div>
